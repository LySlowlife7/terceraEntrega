  <title>FunTime</title>
    <link rel="stylesheet" href="signUp.css">

<p>{{error}}</p>

<body>
    <div id="container">
        <div class="signup-container">
            <h1 class="title">CREAR CUENTA</h1>
            <form  method="post" action="/api/session/signup">
                <div class="register-icons">
                    <img id="icon1"
                        src="data:image/png;base64," title="DISCORD" alt="registrate con: DISCORD"/>
                    <img id="icon2"
                        src="data:image/png;base64," title="FACEBOOK" alt="registrate con: FACEBOOK"/>
                    <img id="icon3"
                        src="data:image/png;base64," title="PINTEREST" alt="registrate con: PINTEREST"/>
                    <img id="icon4"
                        src="data:image/png;base64," title="INSTAGRAM" alt="registrate con: INSTAGRAM"/>
                    <img id="icon5"
                        src="data:image/png;base64," title="YOUTUBE" alt="registrate con: YOUTUBE"/>
                    <img id="icon6"
                        src="data:image/png;base64," title="TWITCH" alt="registrate con: TWITCH"/>
                </div>

                <span class="username">NOMBRE DE USUARIO</span>
                <input type="text" name="first_name" id="signup-usuario">
                <span class="email">CORREO ELECTRÓNICO</span>
                <input type="email" name="email" id="signup-email">
                <span class="password">CONTRASEÑA</span>
                <input type="password" name="password" id="signup-contraseña">
                <input type="button" value="signUp" class="button-signup">
        </div>
    </div>
    </form>

    <div class="container-cuentaCreada">
        <div class="mensajeDeBienvenida">
            <p>Si ya tienes cuenta, inicia sesión aquí</p>
            <button class="button-signup">LOGIN</button>
        </div>
    </div>

    <script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>
    <script src="signUp.js"></script>
</body>